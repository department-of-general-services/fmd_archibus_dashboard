<view version="2.0">
	<title>Draft Building Operations Dashboard</title>

	<layout id="main">
		<west split="true"
			initialSize="350" />
		<center />
	</layout>

	<layout id="nested_west"
	containingLayout="main"
		region="west">
		<north split="true"
			initialSize="50%" />
		<center />
	</layout>

	<layout id="nested_center"
		containingLayout="main"
		region="center">
		<north split="true"
			initialSize="50%" />
		<center />
	</layout>

	<panel id="aging_by_trade"
		type="view"
		file="balt-bldgops-bar-aging-by-trade.axvw"
		layout="nested_west"
		region="north" />

	<panel id="metrics_list"
		type="view"
		file="balt-bldgops-grid-30-open-close.axvw"
		layout="nested_west"
		region="center" />

	<panel id="metric_chart"
		type="view"
		file="balt-bldgops-bar-306090-open-backlog.axvw"
		layout="nested_center"
		region="north" />

	<panel id="volume_chart"
		type="view"
		file="balt-bldgops-bar-open-close-cal-month.axvw"
		layout="nested_center"
		region="center" />
</view>
